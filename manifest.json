{
    "name": "CUGradeView SessionID View",
    "version": "1.0.0",
    "description": "View your sessionID to be used in CUGradeView app",
    "manifest_version": 2,
    "author": "Huzaifa Farooq",
    "browser_action":{
        "default_popup": "index.html",
        "default_title": "CUGradeView SessionID View"
    },
    "permissions": ["cookies","storage", "tabs", "http://*/*", "https://*/*" , "nativeMessaging"],
    "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com; object-src 'self'",
    "icons": { "16": "icon.png", "48": "icon.png", "128": "icon.png" }
}